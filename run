make

query=$1

if [ "$#" -ne 1 ]; then
    query=all
fi

echo $query

for i in `seq 1 10`;
do
    echo $i
    echo "hash"
    ./csv_to_arrow $query hash skew > scripts/data/hash_enum_$i
    echo "lip"
    ./csv_to_arrow $query lip skew > scripts/data/lip_enum_$i
    echo "xiating"
    ./csv_to_arrow $query xiating skew > scripts/data/xiating_enum_$i
    echo "resur"
    ./csv_to_arrow $query resur skew > scripts/data/resur_enum_$i
    echo "lipk"
    ./csv_to_arrow $query lipk skew > scripts/data/lipk_enum_$i
done

python3 ./scripts/plot.py